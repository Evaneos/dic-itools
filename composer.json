	"bin" : [
		"app/dic-itools"
	],
	"config" : {
		"bin-dir" : "bin"
	},
	"require-dev" : {
		"squizlabs/php_codesniffer" : "~1.5",
		"phpunit/phpunit" : "~3.7"
	}
}