{
	"name" : "evaneos/dic-itools",
	"description" : "Tools for DIC-IT library",
	"version" : "1.0.0",
	"type" : "package",
	"homepage" : "http://github.com/thibaud-evaneos/dic-itools",
	"time" : "2013-11-01",
	"license" : "MIT",
	"authors" : [{
			"name" : "Thibaud Fabre",
			"email" : "thibaud@evaneos.com",
			"homepage" : "http://www.evaneos.com",
			"role" : "Developer"
		}
	],
	"autoload" : {
		"psr-4" : {
			"DICIT\\Tools\\Commands\\" : "app/Commands",
			"DICIT\\Tools\\" : "src/"
		}
	},
	"require" : {
		"oliviermadre/dic-it" : "dev-master@dev",
		"symfony/console" : "~2",
		"symfony/yaml" : "2.5.x-dev",
		"psr/log" : "~1"
